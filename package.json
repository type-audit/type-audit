{
  "name": "type-audit",
  "version": "0.9.1",
  "deprecated": false,
  "description": "Check the types of variables, arguments and other values where necessary",
  "main": "dist/type-audit.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint src --color --format stylish",
    "lint:fix": "eslint src --color --format stylish --fix",
    "test": "jest",
    "test:detail": "jest --verbose",
    "test:cover": "jest --coverage",
    "build": "babel src --out-dir dist --presets @babel/preset-env",
    "prepublishOnly": "npm run build"

  },
  "author": "victoraspirin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/type-audit/type-audit.git"
  },
  "homepage": "https://github.com/type-audit/type-audit",
  "bugs": {
    "url": "https://github.com/type-audit/type-audit/issues"
  },
  "keywords": [
    "type",
    "check"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.9.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
